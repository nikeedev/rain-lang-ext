{
  "name": "rain-lang-ext",
  "publisher": "nikeedev",
  "displayName": "Extension for Rain Lang",
  "description": "Extension for working with the Rain programming language",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "icons/Rain-Logo-Light.png",
  "contributes": {
    "languages": [
      {
        "id": "rain",
        "aliases": [
          "Rain",
          "rain"
        ],
        "extensions": [
          "rain"
        ],
        "icon": {
          "dark": "icons/New-Rain-File-Icon.png",
          "light": "icons/New-Rain-File-Icon.png"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rain",
        "scopeName": "source.rain",
        "path": "./syntaxes/rain.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "url": "https://github.com/nikeedev/rain-lang-ext"
  }
}
